        "@wxt-dev/module-react": "^1.1.3",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.3.3",
        "wxt": "^0.20.6"
